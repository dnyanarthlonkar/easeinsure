
               <!-- /. NAV SIDE  -->
        <div id="page-wrapper" >
            <div id="page-inner">
                <div class="row">
                    
                    <div class="col-lg-6 col-md-12">
                        <table width="100%" border="0" cellspacing="0" cellpadding="0">
                          <tr>
                            <td>
                              <%= link_to "New Agent", new_admin_user_path(type: 'agent'), class: "btn btn-primary" %>
                            </td>
                          </tr>
                        </table>
                        <br>
                        <table class="table table-striped table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>First Name</th>
                                    <th>Last Name</th>
                                    <th>Username</th>
                                    <th>Email</th>
                                    <th>Code Number</th>
                                    <th>Address</th>
                                    <th>Contact Number</th>
                                    <th>Designation</th>
                                    <th>Role</th>
                                    <th>Actions</th>
                                </tr>
                            </thead>
                           <% @users.each do |user,index |  %>
                            <tbody>
                              <tr>
                                <td><%= user.try(:username) %></td>
                                <td><%= user.try(:first_name) %></td>
                                <td><%= user.try(:last_name) %></td>
                                <td><%= user.try(:email) %></td>
                                <td><%= user.agent_detail.try(:code_number) %></td>
                                <td><%= user.agent_detail.try(:address) %></td>
                                <td><%= user.agent_detail.try(:contact_number) %></td>
                                <td><%= user.agent_detail.try(:designation) %></td>
                                <td><%= user.roles.first.try(:name) %></td>
                                <td><%= link_to "Edit", edit_admin_user_path(user, type: 'agent') , :class => "btn"%>
                                  | <%= link_to "Delete", admin_user_path(user), method: :delete, data: { confirm: 'Are you sure?' } %></td>
                              </tr>
                            </tbody>
                        <% end %> 
                        </table>

                    </div>
                  </div>
                </div>
              </div>